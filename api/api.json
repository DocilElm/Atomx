    "RegexData": {
        "Dungeons": {
            "Floor": {
                "pattern": "^ +⏣ The Catacombs \\(([\\w\\d].{1,2})\\)$",
                "flags": "" 
            },
            "RoomID": {
                "pattern": "^\\d[\\d\\/]+ [\\w\\d]+ ([-\\d,]+)$",
                "flags": ""
            },
            "PlayerInformation": {
                "pattern": "^\\[[\\d]+\\] ([A-z]+)(?:.+?)? \\(([\\w]+) ([IVXLCDM]+)\\)$",
                "flags": ""
            },
            "SecretsFound": {
                "pattern": "^ Secrets Found: ([\\d,.]+)\\%$",
                "flags": ""
            },
            "Milestone": {
                "pattern": "^ Your Milestone: ☠(.+)$",
                "flags": ""
            },
            "CompletedRooms": {
                "pattern": "^ Completed Rooms: ([\\d]+)$",
                "flags": ""
            },
            "TeamDeaths": {
                "pattern": "^ Team Deaths: ([\\d]+)$",
                "flags": ""
            },
            "PuzzlesAmount": {
                "pattern": "^Puzzles: \\(([\\d]+)\\)$",
                "flags": ""
            },
            "CryptsAmount": {
                "pattern": "^ Crypts: ([\\d]+)$",
                "flags": ""
            }
        },
        "AccountStats": {
            "ProfileName": {
                "pattern": "^Profile\\: ([\\w ]+)$",
                "flags": ""
            },
            "PlayerBank": {
                "pattern": "^ Bank\\: ([\\w\\,.\/]+)",
                "flags": ""
            },
            "Skills": {
                "pattern": "^Skills\\: ([\\w\\d ]+)\\: ([\\w\\d,. ]+)\\%",
                "flags": ""
            },
            "Stats": {
                "pattern": "^ ([\\w ]+)\\: (❁|☣|☠|⚔|✦|☘)([\\d,]+)$",
                "flags": ""
            }
        },
        "Garden": {
            "CropMilestone": {
                "pattern": "^ Milestone\\: ([\\w\\d ]+)\\: ([\\d,.]+)%$",
                "flags": ""
            },
            "JacobContest": {
                "pattern": "^ Starts In\\: ([\\w\\d ]+)$",
                "flags": ""
            },
            "NextVisitor": {
                "pattern": "^Visitors\\: \\((.+)\\)$",
                "flags": ""
            },
            "OrganicMatter": {
                "pattern": "^ Organic Matter\\: ([\\w\\d,.]+)$",
                "flags": ""
            },
            "Fuel": {
                "pattern": "^ Fuel\\: ([\\w\\d,.]+)$",
                "flags": ""
            },
            "TimeLeft": {
                "pattern": "^ Time Left\\: ([\\w\\d ]+)$",
                "flags": ""
            },
            "StoredCompost": {
                "pattern": "^ Stored Compost\\: ([\\w\\d,. ]+)$",
                "flags": ""
            }
        }
    }
