    "RegexData": {
        "Dungeons": {
            "Floor": {
                "pattern": "^ +⏣ The Catacombs \\(([\\w\\d].{1,2})\\)$",
                "flags": "" 
            },
            "RoomID": {
                "pattern": "^\\d[\\d\\/]+ [\\w\\d]+ ([-\\d,]+)$",
                "flags": ""
            },
            "PlayerInformation": {
                "pattern": "^\\[[\\d]+\\] ([A-z]+)(?:.+?)? \\(([\\w]+) ([IVXLCDM]+)\\)$",
                "flags": ""
            },
            "SecretsFound": {
                "pattern": "^ Secrets Found: ([\\d,.]+)\\%$",
                "flags": ""
            },
            "Milestone": {
                "pattern": "^ Your Milestone: ☠(.+)$",
                "flags": ""
            },
            "CompletedRooms": {
                "pattern": "^ Completed Rooms: ([\\d]+)$",
                "flags": ""
            },
            "TeamDeaths": {
                "pattern": "^ Team Deaths: ([\\d]+)$",
                "flags": ""
            },
            "PuzzlesAmount": {
                "pattern": "^Puzzles: \\(([\\d]+)\\)$",
                "flags": ""
            },
            "PuzzleHandler": {
                "pattern": "^ ([A-z ]+)\\: \\[([✦|✔|✖])\\] ?\\(?([A-z]+)?\\)?$",
                "flags": ""
            },
            "CryptsAmount": {
                "pattern": "^ Crypts: ([\\d]+)$",
                "flags": ""
            }
        },
        "AccountStats": {
            "ProfileName": {
                "pattern": "^Profile\\: ([\\w ]+)$",
                "flags": ""
            },
            "PlayerBank": {
                "pattern": "^ Bank\\: ([\\w\\,.\/]+)",
                "flags": ""
            },
            "Skills": {
                "pattern": "^Skills\\: ([\\w\\d ]+)\\: ([\\w\\d,. ]+)\\%",
                "flags": ""
            },
            "Stats": {
                "pattern": "^ ([\\w ]+)\\: (❁|☣|☠|⚔|✦|☘)([\\d,]+)$",
                "flags": ""
            }
        },
        "Garden": {
            "CropMilestone": {
                "pattern": "^ Milestone\\: ([\\w\\d ]+)\\: ([\\d,.]+)%$",
                "flags": ""
            },
            "JacobContest": {
                "pattern": "^ Starts In\\: ([\\w\\d ]+)$",
                "flags": ""
            },
            "NextVisitor": {
                "pattern": "^Visitors\\: \\((.+)\\)$",
                "flags": ""
            },
            "OrganicMatter": {
                "pattern": "^ Organic Matter\\: ([\\w\\d,.]+)$",
                "flags": ""
            },
            "Fuel": {
                "pattern": "^ Fuel\\: ([\\w\\d,.]+)$",
                "flags": ""
            },
            "TimeLeft": {
                "pattern": "^ Time Left\\: ([\\w\\d ]+)$",
                "flags": ""
            },
            "StoredCompost": {
                "pattern": "^ Stored Compost\\: ([\\w\\d,. ]+)$",
                "flags": ""
            }
        },
        "ItemHandler": {
            "Essence": {
                "pattern": "(Undead|Wither) Essence x(\\d+)",
                "flags": ""
            }
        },
        "Party": {
            "MemberAmount": {
                "pattern": "^Party Members \\((\\d+)\\)$",
                "flags": ""
            },
            "PartyMembers": {
                "pattern": "^Party (Members|Moderators)\\: (.+ ●) ?$",
                "flags": ""
            }
        },
        "PartyArray": {
            "NoParty": [
                "^You are not currently in a party\\.$",
                "^You have been kicked from the party by (?:\\[[A-z\\+]+\\])? ?([\\w]{1,16})$",
                "^The party was disbanded because the party leader disconnected\\.$",
                "^The party was disbanded because all invites expired and the party was empty\\.$",
                "^You left the party\\.$"
            ],
            "PartyLeader": [
                "^Party Leader\\: (?:\\[[A-z\\+]+\\])? ?([\\w]{1,16}) ●$",
                "^The party was transferred to (?:\\[[A-z\\+]+\\])? ?([\\w]{1,16}) by .+$",
                "^You have joined (?:\\[[A-z\\+]+\\])? ?([\\w]{1,16})\\'s party\\!$",
                "^(?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) invited .+ to the party\\! They have 60 seconds to accept\\.$"
            ],
            "PlayerJoined": [
                "^(?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) joined the party\\.$",
                "^.+ invited (?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) to the party\\! They have 60 seconds to accept\\.$"
            ],
            "PlayerLeft": [
                "^(?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) has been removed from the party\\.$",
                "^(?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) has left the party\\.$",
                "^(?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) was removed from your party because they disconnected\\.$",
                "^Kicked (?:\\[[A-z]+\\++\\])? ?([\\w]{1,16}) because they were offline\\.$"
            ]
        },
        "ThePlayer": {
            "Purse": {
                "pattern": "^Purse\\: ([\\d,.]+) ?(?:[\\(\\)\\d\\+,.]+)?$",
                "flags": ""
            },
            "Bits": {
                "pattern": "^Bits\\: ([\\d,.]+) ?(?:[\\(\\)\\d\\+,.]+)?$",
                "flags": ""
            },
            "Copper": {
                "pattern": "^Copper\\: ([\\d,.]+) ?(?:[\\(\\)\\d\\+,.]+)?$",
                "flags": ""
            },
            "Motes": {
                "pattern": "^Motes\\: ([\\d,.]+) ?(?:[\\(\\)\\d\\+,.]+)?$",
                "flags": ""
            },
            "Health": {
                "pattern": "^(\\d[,\\d]+)\\/([\\d,]+)❤",
                "flags": ""
            },
            "Defense": {
                "pattern": "([\\d,]+)❈ Defense",
                "flags": ""
            },
            "Mana": {
                "pattern": "(\\d[\\d,]+)\\/([\\d,]+)✎",
                "flags": ""
            },
            "OverflowMana": {
                "pattern": "(\\d[\\d,.]+)ʬ",
                "flags": ""
            },
            "PlayerStats": {
                "pattern": "^ . ([A-z ]+) ([\\d,.\\%]+)$",
                "flags": ""
            },
            "SkillAverage": {
                "pattern": "^([\\d,.]+) Skill Avg\\. \\(non-cosmetic\\)$",
                "flags": ""
            },
            "Collection": {
                "pattern": "^ *(\\d+)\/(\\d+)$",
                "flags": ""
            },
            "BitsAvailable": {
                "pattern": "^Bits Available\\: ([\\d,.]+)$",
                "flags": ""
            },
            "CookieDuration": {
                "pattern": "^Duration\\: (.+)$",
                "flags": ""
            },
            "SelectedPet": {
                "pattern": "^Selected pet\\: ([A-z ]+) ?✦?$",
                "flags": ""
            }
        }
    }
}